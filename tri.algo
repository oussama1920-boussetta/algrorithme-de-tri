ALGORITHM Tri
VAR
    (A,B,C):=entier
    i:=entier
BEGIN
    pour (i allant de 1 pas i-1 )
    LIRE (A.(length[i]))
    si A<B et A<C 
    i(A):=0
    si non
    end
    pour (i allant de 1 pas i-1 )
    LIRE (B.(length[i]))
    si B>A et B<C 
    i(B):=1
    si non
    end
    pour (i allant de 1 pas i-1 )
    LIRE (B.(length[i]))
 
    B.(length[i])
    ecrire 
    B:=3
    
END